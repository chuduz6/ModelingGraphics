<scene>
  <camera>
    <origin>0 0 -2</origin>
    <z>0 0 -1</z>
    <y>0 1 0</y>
    <yfov>45</yfov>
    <xfov>45</xfov>
    <xResolution>512</xResolution>
    <yResolution>512</yResolution>
  </camera>
  <lights>
    <light class="PointLight">
      <intensity>0.8 0.8 0.8</intensity>
      <position>2 4 2</position>
    </light>
    <light class="PointLight">
      <intensity>0.2 0.2 0.2</intensity>
      <position>0 0 5</position>
    </light>
  </lights>
  <hierarchyRoot class="Transform">
    <translation>0 0 0</translation>
    <rotation>0 0 0</rotation>
    <scale>1 1 1</scale>
  	<children>
   	  <child class="Transform">
        <translation>0 -0.75 0</translation>
        <rotation>0 0 0</rotation>
        <scale>0.75 0.75 0.75</scale>
  	    <children>
	  	  <child class="RevolvedSurface">
	  	    <material class="Lambert">
	  	      <diffuse>1 1 1</diffuse>
	  	    </material>
	  	    <profile class="BezierSpline">
	  	      <controlPoints>
	  	        0   0   0
	  	        1.5   0   0
	  	        1.5   2   0
	  	        0   2   0
	  	      </controlPoints>
	  	    </profile>
	  	  </child>
	  	</children>
	  </child>
  	</children>
  </hierarchyRoot>
</scene>
